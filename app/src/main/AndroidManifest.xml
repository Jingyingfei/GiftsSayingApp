        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mainpackage.StartMainActivity" />
        <activity android:name=".mainpackage.WelcomActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mainpackage.LoginActivity" />
        <activity android:name=".classifypackage.main.EditActivity" />
        <activity android:name=".homepackage.selection.HomeItemActivity" />
        <activity android:name=".homepackage.HomeQueryActivity" />
        <activity android:name=".hotspotpackage.recommend.RecommendItemActivity"></activity>
        <activity android:name=".mepackage.SettingActivity"/>
    </application>

</manifest>